{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "FRONTEND_URL": "${VERCEL_URL}"
  },
  "installCommand": "cd ../.. && pnpm install",
  "buildCommand": "pnpm build"
}
